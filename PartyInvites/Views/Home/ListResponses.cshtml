@model IEnumerable<PartyInvites.Models.GuestResponse>

@{ 
    Layout = null;
}

<!DOCTYPE= html>
<html>
<head>
    <meta name="viewport" content="width= device-width" />
    <title>Responses</title>
</head>
<body>
    <h2>Here's the list of the people attending the party:</h2>
    <table>
        <thead>
            <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            </tr>

        </thead>
        <tbody>
            @foreach(PartyInvites.Models.GuestResponse x in Model)
            {
            <tr>
                <td>@x.Name</td>
                <td>@x.Email</td>
                <td>@x.Phone</td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>
